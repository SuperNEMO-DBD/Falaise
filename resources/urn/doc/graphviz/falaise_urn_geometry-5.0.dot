      <tr><td PORT="GEOM"                       align="right" >geometry : geomsetup</td></tr>
      <tr><td PORT="PATH"  bgcolor="lightgreen" align="right" >path</td></tr>
    </table>
    >; ];

  };

  Geometry_Variants_Service_2_0:VARREG -> Geometry_Variants_2_0:MAIN:e;
  Geometry_5_0:VARIANTS                -> Geometry_Variants_Service_2_0:MAIN:e[ color="magenta"; ];
  Geometry_5_0_Services:GEOM           -> Geometry_5_0:MAIN:e; 

