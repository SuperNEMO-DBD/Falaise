subgraph clusterUrn_Simulation {
  label="Simulation";
  fontname="bold";
  bgcolor="lightyellow"; 

  subgraph clusterUrn_Simulation_Variants_2_0 {
    fontname="bold";
    label="Variants";
    bgcolor="white"; 
 		
    /**************************************************/
    Simulation_Variants_2_0 [ shape="plain" fontname="Courier" label = <
    <table border="1">
      <tr><td PORT="MAIN" bgcolor="cyan"       align="left"  >urn:snemo:demonstrator:simulation:variants:2.0</td></tr>
      <tr><td             bgcolor="lightgrey"  align="left"  >category: varregistry</td></tr>
      <tr><td PORT="PATH" bgcolor="lightgreen" align="right" >path</td></tr>
    </table>
    >; ];
  };
  
  subgraph clusterUrn_Simulation_Variants_Service_2_0 {
    label="Simulation Variants Service";
    bgcolor="white"; 

    /**************************************************/
    Simulation_Variants_Service_2_0_Profile_1 [ shape="plain" fontname="Courier" label = <
    <table border="1">
      <tr><td PORT="MAIN"     bgcolor="cyan"       align="left"  >urn:snemo:demonstrator:simulation:variants:service:2.0:profiles:1.0</td></tr>  
      <tr><td                 bgcolor="lightgrey"  align="left"  >category: varprofile</td></tr>
      <tr><td PORT="PATH"     bgcolor="lightgreen" align="right" >path</td></tr>
    </table>
    >; ];
   
    /**************************************************/
    Simulation_Variants_Service_2_0_DefProfile [ shape="plain" fontname="Courier" label = <
    <table border="1">
      <tr><td PORT="MAIN" bgcolor="cyan"       align="left"  >urn:snemo:demonstrator:simulation:variants:service:2.0:profiles:default</td></tr>
      <tr><td             bgcolor="lightgrey"  align="left"  >category: varprofile</td></tr>
      <tr><td PORT="ALIAS"                     align="right" >alias_of : varprofile</td></tr>
    </table>
    >; ];
   
    Simulation_Variants_Service_2_0_GDM [ shape="plain" fontname="Courier" label = <
    <table border="1">
      <tr><td PORT="MAIN" bgcolor="cyan"       align="left"  >urn:snemo:demonstrator:simulation:variants:service:2.0:gdm</td></tr>
      <tr><td             bgcolor="lightgrey"  align="left"  >category: vargdm</td></tr>
      <tr><td PORT="VARDEPS"                   align="right" >vardeps : varregistry*</td></tr>
      <tr><td PORT="PATH" bgcolor="lightgreen" align="right" >path</td></tr>
    </table>
    >; ];
		
    /**************************************************/
    Simulation_Variants_Service_2_0 [ shape="plain" fontname="Courier" label = <
    <table border="1">
      <tr><td PORT="MAIN"       bgcolor="cyan"       align="left"  >urn:snemo:demonstrator:simulation:variants:service:2.0</td></tr>
      <tr><td                   bgcolor="lightgrey"  align="left"  >category: varservice</td></tr>
      <tr><td PORT="VARREG"                          align="right" >registries : varregistry*</td></tr>
      <tr><td PORT="VARPROFS"                        align="right" >profiles : varprofile*</td></tr>
      <tr><td PORT="DEFVARPROF"                      align="right" >defprofile : varprofile</td></tr>
      <tr><td PORT="VARGDM"                          align="right" >gdm : vargdm</td></tr>
      <tr><td PORT="PATH"       bgcolor="lightgreen" align="right" >path</td></tr>
    </table>
    >; ];		

   
    Simulation_Variants_Service_2_0_DefProfile:ALIAS -> Simulation_Variants_Service_2_0_Profile_1:MAIN;
    Simulation_Variants_Service_2_0:VARPROFS         -> Simulation_Variants_Service_2_0_Profile_1:MAIN;
    Simulation_Variants_Service_2_0:DEFVARPROF       -> Simulation_Variants_Service_2_0_DefProfile:MAIN;
    Simulation_Variants_Service_2_0:VARGDM           -> Simulation_Variants_Service_2_0_GDM:MAIN;
  
  };
  
  subgraph clusterUrn_Simulation_Config_2_4 {
    fontname="bold";
    label="Configuration";
    bgcolor="white"; 

    /**************************************************/
    Simulation_2_4 [ shape="plain" fontname="Courier" label = <
    <table border="1">
      <tr><td PORT="MAIN"   bgcolor="cyan"       align="left"  >urn:snemo:demonstrator:simulation:2.4</td></tr>
      <tr><td               bgcolor="lightgrey"  align="left"  >category: simsetup</td></tr>
      <tr><td PORT="VARIANTS"                    align="right" >variants : varservice*</td></tr>
      <tr><td PORT="EXPSETUP"                    align="right" >setup : expsetup</td></tr>
      <tr><td PORT="DECAYSETUP"                  align="right" >decays : decaysetup</td></tr>
      <tr><td PORT="VERTEXSETUP"                 align="right" >vertex : vertexsetup</td></tr>
      <tr><td PORT="SERVICES"                    align="right" >services : services</td></tr>
      <tr><td PORT="PATH"   bgcolor="lightgreen" align="right" >path</td></tr>
    </table>
    >; ];

  };
    
  Simulation_Variants_Service_2_0:VARREG -> Simulation_Variants_2_0:MAIN;
  Simulation_2_4:VARIANTS                -> Simulation_Variants_Service_2_0:MAIN[ color="magenta"; ];
  
};

Simulation_2_4:EXPSETUP     -> Setup_2_0:MAIN;
Simulation_2_4:DECAYSETUP   -> Decays_1_4:MAIN;
Simulation_2_4:VERTEXSETUP  -> Vertex_5_0:MAIN;
Simulation_2_4:SERVICES     -> Setup_2_0_Services:MAIN;
Simulation_Variants_Service_2_0:VARREG -> Geometry_Variants_2_0:MAIN;
Simulation_Variants_Service_2_0:VARREG -> Vertex_Variants_1_0:MAIN;
Simulation_Variants_Service_2_0:VARREG -> Decays_Variants_1_0:MAIN;
Simulation_Variants_Service_2_0_GDM:VARDEPS -> Geometry_Variants_2_0:MAIN;
